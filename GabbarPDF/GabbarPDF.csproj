<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net10.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>

		
		<PackageId>GabbarPDF</PackageId>
		<Version>1.0.0</Version>
		<Authors>Kitne Aadmi The?</Authors>
		<Description>Jo Gabbar se PDF mangega... woh PDF hi milega!</Description>
		<PackageTags>gabbar pdf hindi</PackageTags>
		
		
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
	</PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="Fonts\NotoSansDevanagari-Regular.ttf" />
	</ItemGroup>

	<ItemGroup>
		<None Update="gabbar.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Update="readme.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>